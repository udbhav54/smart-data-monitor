# Development environment
NODE_ENV=development

# API Configuration
VITE_APP_NAME=Smart Data Monitor
VITE_APP_VERSION=1.0.0

# Debug settings
VITE_DEBUG_MODE=true
VITE_LOG_API_CALLS=true

# Data simulation settings
VITE_SIMULATION_INTERVAL=5000
VITE_MAX_DATA_POINTS=20

# Alert thresholds
VITE_HIGH_USAGE_THRESHOLD=80
VITE_LOW_SPEED_THRESHOLD=1

# Location settings
VITE_LOCATION_TIMEOUT=10000
VITE_LOCATION_MAX_AGE=600000